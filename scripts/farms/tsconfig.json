{
  "extends": "@sushiswap/typescript-config/node16.json",
  "include": [".graphclient", "src"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": ".",

    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "node16",
    "esModuleInterop": true
  },
  "resolveJsonModule": true,
  "ts-node": {
    "esm": true
  }
}